RevoluteJoints: 
  RevoluteJoint : 
    Name : rvj_pitch_controller_1 ; 
    ConnectedTo : blade_1_1 ; 
    Where : pt_blade_root_1_1 ; 
    ConnectedTo : rb_blade_fixed_root_1 ; 
    Where : pt_blade_root_1_1 ; 
    Triad : rvj_pitch_command_1 ; 
    RelativeRotation :  
      Name : pitch_rel_rot_1 ; 
      ConnectedTo : pres_pitch_rel_rot_1 ; 
      Where : pt_blade_root_1_1 ; 
     ;
   ; 
 ; 


PrescribedDisplacements : 
  PrescribedDisplacement :
    Name : pres_pitch_rel_rot_1 ; 
    AppliedTo : pitch_rel_rot_1 ; 
    Where : pt_blade_root_1_1 ; 
    DegreeOfFreedom : 4 ; 
    TimeFunction : blade_pitch_control_1 ; 
#    Controller : Controller_4_Wind_Turbine  ; 
   ; 
 ; 


 Triads : 
   Triad :
     Name : rvj_pitch_command_1 ; 
     YVector :  0 ,  1 ,  0 ; 
     ZVector :  1 ,  0 ,  0 ; 
     InReferenceFrame : blade_root_frame_1 ; 
   ;
 ; 


